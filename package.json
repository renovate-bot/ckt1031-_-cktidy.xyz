{
    "private": true,
    "name": "cktidy",
    "version": "0.0.26",
    "description": "Cktsun's Personal Website",
    "license": "MIT",
    "author": "cktsun1031",
    "type": "module",
    "sideEffects": false,
    "homepage": "https://github.com/cktsun1031/cktidy#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cktsun1031/cktidy.git"
    },
    "bugs": {
        "url": "https://github.com/cktsun1031/cktidy/issues"
    },
    "scripts": {
        "build": "next build && next-sitemap",
        "commit": "cz",
        "dev": "next dev -p 4072",
